{
  "name": "fictum",
  "version": "0.0.6",
  "description": "fictum lib",
  "main": "dist/index.js",
  "engines": {
    "node": "14.18.3",
    "npm": "6.14.15"
  },
  "scripts": {
    "build": "rimraf dist && ttsc -b tsconfig.build.json",
    "build:watch": "npm run build -- -w",
    "eslint": "eslint 'src/**' --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand --silent --noStackTrace --collectCoverage=false",
    "test:verbose": "jest --passWithNoTests --no-cache --runInBand",
    "test:unit": "npm test -- -c jest.config.js --watch",
    "test:staged": "npm test -- --findRelatedTests tests",
    "test:ci": "npm test -- --coverage"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leal32b/fictum.git"
  },
  "keywords": [
    "fictum",
    "fake",
    "faker",
    "data",
    "generator",
    "test",
    "mock",
    "fixtures",
    "random"
  ],
  "author": "leal32b",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leal32b/fictum/issues"
  },
  "homepage": "https://github.com/leal32b/fictum#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "eslint": "^8.13.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.1.1",
    "husky": "4.3.8",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.3",
    "typescript-transform-paths": "^3.3.1"
  }
}
